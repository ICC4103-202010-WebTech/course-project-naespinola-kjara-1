<div class="col-md-4">
  <div class="card border-success mb-3" style="max-width: 20rem;">
    <div class="card-header bg-success text-white border-success"><%= dates_to_vote.event.title %></div>
    <div class="card-body text-success">
      <h5 class="card-title">Date</h5>
      <p class="card-text"><%= link_to dates_to_vote.date, dates_to_vote %></p>
    </div>
    <div class="card-footer text-muted bg-transparent border-success"> Created: <%= dates_to_vote.created_at%></div>
  </div>
</div>

