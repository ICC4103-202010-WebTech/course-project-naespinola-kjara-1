<p id="notice"><%= notice %></p>

<h1>Dates To Votes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dates_to_votes.each do |dates_to_vote| %>
      <tr>
        <td><%= link_to 'Show', dates_to_vote %></td>
        <td><%= link_to 'Edit', edit_dates_to_vote_path(dates_to_vote) %></td>
        <td><%= link_to 'Destroy', dates_to_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dates To Vote', new_dates_to_vote_path %>
