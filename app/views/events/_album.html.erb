<div >
  <div class="row ml-0 mt-3">
    <h2 class="text-info">Upcoming Events</h2>
  </div>
  <div class="row">
    <% object.each do |event| %>
      <%= render partial: 'events/event_tile', locals: { event: event } %>
      <%= image_tag(event.image.thumb.url, class: "card-img-top", width: "100%", background: "#55595c", color: "#eceeef") %>
    <% end %>
  </div>
</div>