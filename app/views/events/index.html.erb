<div class="row ml-0 mt-3">
  <h2 class="text-info">🎆 My Events</h2>
</div>
<div class="row ml-0 mt-2">
  <% if (user_signed_in?) || (admin_signed_in?) %>
    <%= link_to "Create Event", new_event_path(@members), class: ["btn", "btn-primary"] %>
  <% end %>
</div>
<div class="row">
  <% @events.each do |event| %>
    <%= render partial: 'event_tile', locals: { event: event } %>
  <% end %>
</div>
<br>
<br>