<%= form_with model: [@organization, @member], local: true do |form| %>
  <%= errors_for @member %>
  <div class="form-group">
    <%= form.label :user_id , class:"text-success font-weight-bold"%>
    <%= form.collection_select(:user_id, User.all.order(username: :asc)-Member.all, :id, :username, {}, { class: "form-control" }) %>


  </div>
  <div class="form-group">
    <%= form.submit class: ["btn", "btn-primary"] %>
    <%= link_to "Back", :back, class: ["btn", "btn-secondary"] %>
  </div>
<% end %>