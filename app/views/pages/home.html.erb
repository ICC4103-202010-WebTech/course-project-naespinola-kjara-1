<div >
  <div class="row ml-0 mt-3">
    <h2 class="text-info">Upcoming Events</h2>
  </div>
  <div class="row">
    <% @events.each do |event| %>
      <%= render partial: 'event_tile', locals: { event: event } %>
    <% end %>
  </div>
  <div class="row ml-0 mt-3">
    <h2 class="text-info">Organizations </h2>
  </div>
  <div class="row">
    <% @organizations.each do |organization| %>
      <%= render partial: 'organization_tile', locals: { organization: organization } %>
    <% end %>
  </div>
</div>